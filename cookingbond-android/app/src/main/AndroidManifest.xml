<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sweetheart.cookingbond">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity
            android:name=".CookMainActivity"
            android:label="@string/title_activity_cook_main"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".AddDishActivity"
            android:parentActivityName=".CookMainActivity" />
        <activity
            android:name=".CookChatActivity"
            android:parentActivityName=".CookMainActivity" />
        <activity android:name=".EaterMainActivity"></activity>
    </application>

</manifest>